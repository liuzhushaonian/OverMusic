<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="OverMusicTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowNoTitle">true</item>


    </style>

    <style name="Material" parent="@android:style/Theme.Material.Light.NoActionBar">
        <item name="windowNoTitle">true</item>

    </style>

    <style name="BottomStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <!--<item name="windowNoTitle">true</item>-->
        <item name="android:windowBackground">@android:color/transparent</item>

    </style>

    <style name="Buttom">
        <item name="android:background">@drawable/select_theme</item>
        <item name="android:tint">@color/colorPurple</item>

    </style>

</resources>
